saMtaCallStatLast24HrsTable   OBJECT-TYPE
    SYNTAX        SEQUENCE OF SaMtaCallStatLast24HrsEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
       "This table contains last 24 hrs calls statistics."
   ::=  { saMtaStatsBase 7 }

saMtaCallStatLast24HrsEntry  OBJECT-TYPE
    SYNTAX        SaMtaCallStatLast24HrsEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "List of individual parameters for a single call"
    INDEX { ifIndex }
    ::= { saMtaCallStatLast24HrsTable 1 }

SaMtaCallStatLast24HrsEntry   ::= SEQUENCE {
    saMtaCallStatLast24HrsAvgJitterMeasurement   INTEGER,
    saMtaCallStatLast24HrsMaxJitterMeasurement   INTEGER,
    saMtaCallStatLast24HrsRTPAvgLatency          INTEGER,
    saMtaCallStatLast24HrsMGCPResponseAvgLatency INTEGER,
    saMtaCallStatLast24HrsSuccessfulMsgCount     INTEGER,
    saMtaCallStatLast24HrsNegAckCount            INTEGER,
    saMtaCallStatLast24HrsNoNTFYAckCount         INTEGER
 
}
 
saMtaCallStatLast24HrsAvgJitterMeasurement OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This objects contains the average jitter measurements for the last 24 hours."
    ::= { saMtaCallStatLast24HrsEntry 1 }
 
saMtaCallStatLast24HrsMaxJitterMeasurement OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This objects contains the maximum jitter measurements for the last 24 hours."
    ::= { saMtaCallStatLast24HrsEntry  2 }
 
saMtaCallStatLast24HrsRTPAvgLatency OBJECT-TYPE
    SYNTAX      INTEGER
    UNITS       "milliseconds"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This objects contains the average latency/delay for RTP Packets for the last
        24 hours."
    ::= { saMtaCallStatLast24HrsEntry  3 }
 
saMtaCallStatLast24HrsMGCPResponseAvgLatency OBJECT-TYPE
    SYNTAX      INTEGER
    UNITS       "milliseconds"
    MAX-ACCESS  read-only
