saMtaCallVoipMetricsTable   OBJECT-TYPE
    SYNTAX        SEQUENCE OF SaMtaCallVoipMetricsEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
       "This table contains Voip call metrics."
   ::=  { saMtaStatsBase 8 }

saMtaCallVoipMetricsEntry  OBJECT-TYPE
    SYNTAX        SaMtaCallVoipMetricsEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "List of individual voip metrics for a single call"
    INDEX { saMtaCallVoipMetricsIndex }
    ::= { saMtaCallVoipMetricsTable 1 }

SaMtaCallVoipMetricsEntry   ::= SEQUENCE {    
	  saCallVoipMetricsIndex                    Integer32,
      saCallVoipMetricsMOSScore                 INTEGER,
      saCallVoipMetricsRFSignalToNoiseLevel     Unsigned32,
      saCallVoipMetricsResidualEchoRetLoss      Integer32,
      saCallVoipMetricsIntervalJitter           Unsigned32,
      saCallVoipMetricsAvgTxDelay               Unsigned32,
      saCallVoipMetricsJBDiscardRate            Unsigned32,
      saCallVoipMetricsJBAdjRate                Unsigned32,
      saCallVoipMetricsJBMaxSize                Unsigned32,
      saCallVoipMetricsNetRdTripDelay           Unsigned32,
      saCallVoipMetricsSignalLevel              Integer32,
      saCallVoipMetricsNoiseLevel               Integer32,
      saCallVoipMetricsRfTxLevel                Integer32,
      saCallVoipMetricsRfRxLevel                Integer32,
      saCallVoipMetricsCallTrace                SnmpAdminString
 }           

  saCallVoipMetricsIndex OBJECT-TYPE
    SYNTAX Integer32 (1..20) 
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Index of the saCallVoipMetricsEntry."
    ::= { saMtaCallVoipMetricsEntry 1 } 
 
  saCallVoipMetricsMOSScore OBJECT-TYPE
    SYNTAX      INTEGER {
      bad(1),
      poor(2),
      fair(3),
      good(4),
      excellent(5)
    }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object reports the numerical indication of the perceived quality
        of received media after compression and/or transmission.  The MOS score
        is expressed as a single number in the range of 1 to 5.  The associated
        quality characteristics defined for each MOS score:
        
        bad(1) - Very annoying
        poor(2) - Annoying
        fair(3) - Slightly annoying
        good(4) - Perceptible, but not annoying
        excellent(5) - Imperceptible"
    ::= { saMtaCallVoipMetricsEntry 2 }

saCallVoipMetricsRFSignalToNoiseLevel OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object reports the general measurement of the ratio of RF signal power 
        to noise power."
    ::= { saMtaCallVoipMetricsEntry 3 }

saCallVoipMetricsResidualEchoRetLoss OBJECT-TYPE
    SYNTAX      Integer32
    UNITS "dB"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object reports the residual echo return loss, which relates to the
        effective echo level that a user would hear.  It is the ratio between the
        transmitted signal level and the received echo level expressed in dB."
    ::= { saMtaCallVoipMetricsEntry 4 }

saCallVoipMetricsIntervalJitter OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS "Milliseconds"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object reports an estimate of the statistical variance of the RTP data
        packet inter-arrival time."
    ::= { saMtaCallVoipMetricsEntry 5 }

saCallVoipMetricsAvgTxDelay OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS "Milliseconds"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object reports an estimate of the network latency.  The is the average 
        value of the difference between the NTP timestamp indicated by the senders of
        the RTCP messages and the NTP timestamp of the receivers, measured when the
        messages are received."
    ::= { saMtaCallVoipMetricsEntry 6 }

saCallVoipMetricsJBDiscardRate OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object reports the number of packets discarded by the eMTA's jitter buffer."
    ::= { saMtaCallVoipMetricsEntry 7}

saCallVoipMetricsJBAdjRate OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS "Milliseconds"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object reports the jitter buffer's adjustment rate."
    ::= { saMtaCallVoipMetricsEntry 8}

saCallVoipMetricsJBMaxSize OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS "Milliseconds"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object reports the maximum size of the jitter buffer, in milliseconds."
    ::= { saMtaCallVoipMetricsEntry 9 }

saCallVoipMetricsNetRdTripDelay OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS "Milliseconds"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object reports the round trip delay (latency) for a packet to travel from
        the originating end of a call to the terminating end of a call, and back to the
        originating end of a call."
    ::= { saMtaCallVoipMetricsEntry 10 }

saCallVoipMetricsSignalLevel OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "dB"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object reports the audio signal levels."
    ::= { saMtaCallVoipMetricsEntry 11 }

saCallVoipMetricsNoiseLevel OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "dB"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object reports the audio noise levels."
    ::= { saMtaCallVoipMetricsEntry 12 }

saCallVoipMetricsRfTxLevel OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "dB"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object reports the RF transmit level."
    ::= { saMtaCallVoipMetricsEntry 13 }

saCallVoipMetricsRfRxLevel OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "dB"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object reports the RF receive level."
    ::= { saMtaCallVoipMetricsEntry 14 }

saCallVoipMetricsCallTrace OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object reports the capture of MGCP/NCS message series used to establish
        and terminate call.  The call trace information include the following 
        information:
        (1) The NCS command/message (CRCX, DLCX, MDCX, etc.)
        (2) The transaction identifier (transaction number)
        (3) Any applicable command parameters (CallID, DigitMap, DetectEvents, etc)
        (4) The destination for the command/message (eg aaln/1@broadcom.com)
        (5) The protocol version numbers for both MGCP and NCS"
    ::= { saMtaCallVoipMetricsEntry 15 }
        
saMtaCallStatDialedHash OBJECT-TYPE
    SYNTAX INTEGER {
        none(0),
        all(1),
        last3(2)
        } 
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Mib to control the formatting of the displayed phone number in saMtaCallStatDialedNumber and 
        saMtaCallStatLast10DialedNumber.    
        none(0) : Do not hash out any digits of the recorded phone number
        all(1)  : Hash out all the digits of the recorded phone number
        last3(2): Hash the last three digits of the recorded phone number (ex: 7701234xxx).
        This MIB is read-only, and can only be set in the MTA config file."
    DEFVAL { 0 }
    ::= { saMtaStatsBase 9 }
END

