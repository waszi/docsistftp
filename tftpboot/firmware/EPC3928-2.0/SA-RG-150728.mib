SaRgL2ogreSourceIfEntry ::= SEQUENCE {
    saRgL2ogreSourceIfInstance           Unsigned32,
    saRgL2ogreSourceIf                   INTEGER,
    saRgL2ogreSourceIfEnabled            TruthValue,
    saRgL2ogreSourceIfVlanTag            Integer32,
    saRgL2ogreSourceIfMplsHeader         Integer32,
    saRgL2ogreSourceIfRowStatus          RowStatus
}

saRgL2ogreSourceIfInstance    OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The key for a unique instance of this object."
    ::= {saRgL2ogreSourceIfEntry 1 }

saRgL2ogreSourceIf OBJECT-TYPE
    SYNTAX      INTEGER {
                        wifi1-0(1),
                        wifi1-1(2),
                        wifi1-2(3),
                        wifi1-3(4),
                        wifi1-4(5),
                        wifi1-5(6),
                        wifi1-6(7),
                        wifi1-7(8),
                        wifi2-0(9),
                        wifi2-1(10),
                        wifi2-2(11),
                        wifi2-3(12),
                        wifi2-4(13),
                        wifi2-5(14),
                        wifi2-6(15),
                        wifi2-7(16)
        }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Source interface whose traffic will be pass through GRE tunnel."
    ::= { saRgL2ogreSourceIfEntry 2 }

saRgL2ogreSourceIfEnabled OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Enables or disable mapping traffic from network interface into GRE tunnel."
   ::= { saRgL2ogreSourceIfEntry 3 }

saRgL2ogreSourceIfVlanTag OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Add VLAN tag consists of 16bit of Tag Protocol Identifier (TPID)
         and 16 bit of Tag Control Identifier (TCI).
         Value of 0 will disable adding VLAN tag.
         Tag Protocol Identifier (TPID) should set to 0x8100."
    ::= { saRgL2ogreSourceIfEntry 4 }

saRgL2ogreSourceIfMplsHeader OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "MPLS Header. Value of 0 will disable adding MPLS Header."
    ::= { saRgL2ogreSourceIfEntry 5 }

saRgL2ogreSourceIfRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The RowStatus interlock for the creation and deletion of a table entry."
    ::= { saRgL2ogreSourceIfEntry 6 }

saRgL2ogreOrigIf OBJECT-TYPE
    SYNTAX      INTEGER {
                eRouter(1)
               }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Originating Interface of GRE tunnel."
    ::= { saRgL2ogreBase 13 }

saRgL2ogreConcentratorServiceName OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "FQDN of GRE tunnel concentrator/gw service.
         If this is set then, DNS Query of type SRV will be used for discovering
         FQDN of primary and secondary remote endpoint of GRE tunnel."
    ::= { saRgL2ogreBase 14 }

saRgL2ogreDnsResolverRetryTimerMin OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Minimum timeout interval in seconds before retrying DNS query."
    ::= { saRgL2ogreBase 15 }

saRgL2ogreDnsResolverRetryTimerMax OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Maximum timeout interval in seconds before retrying DNS query."
    ::= { saRgL2ogreBase 16 }

saRgL2ogreKeepAliveFailureInterval OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Configured interval between pings for WLAN GW when disabled due to both WLAN GWs failing."
    ::= { saRgL2ogreBase 17 }

saRgL2ogreKeepAliveReattemptInterval OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Configured interval to reattempt connection to a primary WLAN GW even when secondary WLAN GW is available and clients are connected."
    ::= { saRgL2ogreBase 18 }

saRgIGMP OBJECT IDENTIFIER ::= { saRg 15 }

saRgIGMPMaxRspTime OBJECT-TYPE
    SYNTAX INTEGER {
        IGMPv2(1),
        IGMPv3(2)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "There is a difference in IGMPv2 and IGMPv3 to translate the MAX RSP CODE to MAX RSP TIME.
                 In IGMPv2 “MAX RSP TIME” = “MAX RSP CODE”/10.
                 In IGMPv3, When Max RSP Code > 128, max response time is calculated differently.
                 When this MIB is set to IGMPv2(1) modem will set max response time according to IGMPv2 standard.
                 This setting is recommended for older CMTS implementations and when random response time of 0 to 10 sec is desired.
                 When this MIB is set to IGMPv3(2) modem will set max response time according to IGMPv3 standard.
                 This setting is recommended for new CMTS implementations and when random response time of 0 to above 10 seconds is desired.
         This object is stored in non-vol and defaults to IGMPv2(1) after factory reset."
        DEFVAL { 1 }
    ::= { saRgIGMP 1 }


-- ====================================================
-- Network Extender Status MIB
-- ====================================================

saRgNetworkExtenders OBJECT IDENTIFIER ::= { saRg 16 }

saRgNetworkExtenderTable  OBJECT-TYPE
    SYNTAX      SEQUENCE OF SaRgNetworkExtenderEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Multiple Attached Network Extender Table. Maximum of 8 Entries"

    ::= { saRgNetworkExtenders 1 }

saRgNetworkExtenderEntry OBJECT-TYPE
    SYNTAX      SaRgNetworkExtenderEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A row in the table which represents information about attached Network Extender."
    INDEX   { saRgNetworkExtenderIndex }
    ::= { saRgNetworkExtenderTable 1 }

SaRgNetworkExtenderEntry ::= SEQUENCE {
      saRgNetworkExtenderIndex                INTEGER,
      saRgNetworkExtenderIpAddress            IpAddress,
      saRgNetworkExtenderDeviceName           OCTET STRING,
      saRgNetworkExtenderVenderName           OCTET STRING,
      saRgNetworkExtenderModelNumber          OCTET STRING,
      saRgNetworkExtenderFirmwareVersion      OCTET STRING
    }

saRgNetworkExtenderIndex OBJECT-TYPE
    SYNTAX      INTEGER (1..8)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The index of the the table.  Up to 8 connected devices supported"
    ::= { saRgNetworkExtenderEntry 1 }

saRgNetworkExtenderIpAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The IP Address of the Device"
    ::= { saRgNetworkExtenderEntry 2 }

saRgNetworkExtenderDeviceName OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(0..32))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The Name of the Device as returned by HNAP"
    ::= { saRgNetworkExtenderEntry 3 }

saRgNetworkExtenderVenderName OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(0..32))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The Vender Name as returned by HNAP"
    ::= { saRgNetworkExtenderEntry 4 }

saRgNetworkExtenderModelNumber OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(0..32))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The Model Number of the Device as returned by HNAP"
    ::= { saRgNetworkExtenderEntry 5 }

saRgNetworkExtenderFirmwareVersion OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(0..32))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The Firmware version of the Device as returned by HNAP"
    ::= { saRgNetworkExtenderEntry 6 }

-- ====================================================
-- Network Extender Radio Status Table
-- ====================================================

saRgNetworkExtenderRadioTable  OBJECT-TYPE
    SYNTAX      SEQUENCE OF SaRgNetworkExtenderRadioEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Attached Network Extender Radio Table. Maximum of 32 entries (per extender).
        If the attached device has no radios/SSID, this table will be empty"
    ::= { saRgNetworkExtenders 2 }

saRgNetworkExtenderRadioEntry OBJECT-TYPE
    SYNTAX      SaRgNetworkExtenderRadioEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A row in the table for Network Extender attached WiFi Radios."
    INDEX       {   saRgNetworkExtenderIndex,
                                saRgNetworkExtenderRadioIndex }
    ::= { saRgNetworkExtenderRadioTable 1 }

SaRgNetworkExtenderRadioEntry ::= SEQUENCE {
        saRgNetworkExtenderRadioIndex           INTEGER,
        saRgNetworkExtenderRadioSSID            OCTET STRING,
        saRgNetworkExtenderRadioBSSID           PhysAddress,
        saRgNetworkExtenderRadioChannel         OCTET STRING,
        saRgNetworkExtenderRadioMode            OCTET STRING
    }

saRgNetworkExtenderRadioIndex OBJECT-TYPE
    SYNTAX      INTEGER (1..32)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The index of the radio/SSID. Up to 32 SSIDs supported"
    ::= { saRgNetworkExtenderRadioEntry 1 }

saRgNetworkExtenderRadioSSID OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(0..32))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The SSID of the radio as by HNAP"
    ::= { saRgNetworkExtenderRadioEntry 2 }

saRgNetworkExtenderRadioBSSID OBJECT-TYPE
    SYNTAX      PhysAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The BSSID as reported by HNAP"
    ::= { saRgNetworkExtenderRadioEntry 3 }

saRgNetworkExtenderRadioChannel OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(0..32))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Returns the Primary Radio Channel Number as reported by HNAP"
    ::= { saRgNetworkExtenderRadioEntry 4 }

saRgNetworkExtenderRadioMode OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(0..32))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Returns the Radio Operational Mode as reported by HNAP"
    ::= { saRgNetworkExtenderRadioEntry 5 }

-- ====================================================
-- Network Extender Connected Client Table
-- ====================================================
saRgNetworkExtenderClientTable  OBJECT-TYPE
    SYNTAX      SEQUENCE OF SaRgNetworkExtenderClientEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Attached Network Extender Client Table. Maximum of 64 Entries per Extender"
    ::= { saRgNetworkExtenders 3 }

saRgNetworkExtenderClientEntry OBJECT-TYPE
    SYNTAX      SaRgNetworkExtenderClientEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A row in the table denoting Network Extender Clients."
    INDEX       {   saRgNetworkExtenderIndex,
                    saRgNetworkExtenderClientIndex }
    ::= { saRgNetworkExtenderClientTable  1 }

SaRgNetworkExtenderClientEntry ::= SEQUENCE {
         saRgNetworkExtenderClientIndex          INTEGER,
         saRgNetworkExtenderClientMAC            PhysAddress,
         saRgNetworkExtenderClientInterface      OCTET STRING
    }

saRgNetworkExtenderClientIndex OBJECT-TYPE
    SYNTAX      INTEGER (1..64)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The index of the Client."
    ::= { saRgNetworkExtenderClientEntry 1 }


saRgNetworkExtenderClientMAC OBJECT-TYPE
    SYNTAX      PhysAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Returns the client's MAC as reported by HNAP"
    ::= { saRgNetworkExtenderClientEntry 2 }

saRgNetworkExtenderClientInterface OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(0..32))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Returns the client's connected interface per HNAP.   Wifi-2.4, Wifi-5.0, Ethernet, etc"
    ::= { saRgNetworkExtenderClientEntry 3 }

END
