saRgL2TPv3CookieSize OBJECT-TYPE
    SYNTAX   Integer32{
                noCookie(0),
                size4(1),
                size8(2)
    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "Sets the Cookie Size. for ex: 0 - No Cookie"
    ::= { saRgL2TPv3Client 8 }

saRgL2TPApplySettings OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "If set to true(1), then RG will reboot and apply new L2TP settings.
