saRgL2TP OBJECT IDENTIFIER ::=  { saRg 10 }

saRgL2TPOperMode OBJECT-TYPE
    SYNTAX      INTEGER {
                l2TPDisable(0),
                l2TPv2client(1),
                l2TPv2server(2),
                l2TPv3client(3)
    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "L2TPDisable(0) - Disables l2tp client or server Functionality
         L2TPv2client(1) - Enables l2tpv2 client Functionality
         L2TPv2server(2) - Enables l2tpv2 server Functionality
         L2TPv3client(3) - Enables l2tpv3 client Functionality"
     ::= { saRgL2TP 1 }

-- L2TPv2 Client Mode START

saRgL2TPv2Client OBJECT IDENTIFIER ::=  { saRgL2TP 2 }

saRgL2TPv2PPPServerAddressType OBJECT-TYPE
    SYNTAX      InetAddressType{
        unknown(0),
        ipv4(1),
        ipv6(2)
    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "This MIB defines the type of internet address to be used for the L2TP Server.
            0:  An unknown address type.  This value MUST
                be used if the value of the corresponding
                InetAddress object is a zero-length string.
                It may also be used to indicate an IP address
                that is not in one of the formats defined
                below.
            1 : IPv4 L2TP Server Address
            2 : IPv6 L2TP Server Address (reserved for future support)
            NOTE: The default value of 1 indicates support for IPv4 IP address."
    DEFVAL { ipv4 }
    ::= { saRgL2TPv2Client 1 }

saRgL2TPv2PPPServerAddress OBJECT-TYPE
    SYNTAX      InetAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "IP address of the remote endpoint L2TP server."
    ::= { saRgL2TPv2Client 2 }


saRgL2TPv2PPPUsername OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(0..32))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "PPPUsername as in the PPP Server user list"
    ::= { saRgL2TPv2Client 3 }


saRgL2TPv2PPPPassword OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(0..32))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "PPPPassword set for the user on the PPP Server it is trying to connect"
    ::= { saRgL2TPv2Client 4 }

