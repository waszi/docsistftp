saRgWifiHotspotCapable OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Returns whether the device is capable of WiFi HotSpot functionality."
   ::= { saRgWifiHotspotBase 7 }

saRgWifiHotspotConnectedClientsTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF SaRgWifiHotspotConnectedClientsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Clients table connected to Wi-fi Hotspot."
    ::= { saRgWifiHotspotBase 8 }

saRgWifiHotspotConnectedClientsEntry OBJECT-TYPE
    SYNTAX      SaRgWifiHotspotConnectedClientsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
         "An entry defining the Wi-fi hotspot connected clients"
